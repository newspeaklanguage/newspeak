	should: [array copyStringFrom: nil to: 2] signal: Error.
	should: [array copyStringFrom: 2 to: nil] signal: Error.
	should: [array copyStringFrom: 2 to: 50] signal: Error.
	should: [array copyStringFrom: 0 to: 2] signal: Error.
