protectedMessage: node = (
	sentSelectors add: node selector.
	^js literal: (names mangleProtected: node selector)
)
publicMessage: node = (
	sentSelectors add: node selector.
	^js literal: (names manglePublic: node selector)
)
public receiverNode: node <ReceiverNode> = (
	^js ident: selfName
