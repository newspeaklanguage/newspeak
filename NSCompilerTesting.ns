public testAssignNilToMutableLazySlotForcesToRecomputeExpression = (
	test:
		'class Sample = ( | public counter ::= 0. | ) (
			lazy public testMutable ::= incrementOneAndReturnCounter.
			private incrementOneAndReturnCounter = ( counter:: counter  + 1. ^counter)
		)'
	with:
		'class SampleTest test: sampleClass = ( | sample = sampleClass new. | ) (
			public test = (
				assert: sample counter equals: 0.
				assert: sample testMutable equals: 1.
				assert: sample counter equals: 1.
				sample testMutable: nil.
				assert: sample counter equals: 1.
				assert: sample testMutable equals: 2.
				assert: sample counter equals: 2.
			)
		)'.
)
