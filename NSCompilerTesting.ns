public testIfCascade = (
(* Regression for bitbucket issue 85 *)
	test:
		'class Sample = ( | public slot1 public slot2 | ) (
			public fail = (
				| x |
				x: class new.
				x
					slot1: (true ifTrue: [3] ifFalse: [4]);
					slot2: 8.
				^x
			)
		)'
	with:
		'class SampleTest test: sampleClass = ( | sample = sampleClass new. | ) (
