class NameMangler = (|
	private specialCharacterManglings = Map new.
|specialCharacterManglings
		at: "+" put: '$plus';
		at: "-" put: '$minus';
		at: "*" put: '$times';
		at: "/" put: '$over';
		at: "\" put: '$back';
		at: "<" put: '$less';
		at: ">" put: '$greater';
		at: "~" put: '$tilde';
		at: "=" put: '$equal';
		at: "@" put: '$at';
		at: "%" put: '$percent';
		at: "|" put: '$pipe';
